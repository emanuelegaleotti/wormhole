    "@ngrx/store-devtools": "^15.4.0",
    "@tailwindcss/forms": "^0.5.7",
    "aws-amplify": "^6.0.5",
    "firebase": "^10.7.0",
    "ngx-webstorage": "^11.1.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
