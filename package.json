{
  "name": "wormhole",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prepare": "husky install",
    "wormhole:lint": "ng lint --fix",
    "wormhole:format": "standard --fix",
    "lint": "ng lint",
    "serve:wormhole": "ng serve Wormhole",
    "serve:vehicles": "ng serve vehicles",
    "serve:weather": "ng serve weather",
    "start:wormhole": "ng serve Wormhole -o --port 4200",
    "start:vehicles": "ng serve vehicles -o --port 3000",
    "start:weather": "ng serve weather -o --prot 3300",
    "start:cityWeather": "ng serve cityWeather -o --port 3600",
    "start:all": "concurrently \"npm run start:wormhole\" \"npm run start:vehicles\"  \"npm run start:weather\" \"npm run start:cityWeather\"",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "compodoc": "npx compodoc -p tsconfig.doc.json"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.3.14",
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ngneat/transloco": "^4.2.6",
    "@ngrx/effects": "^14.3.2",
    "@ngrx/store": "^14.3.2",
    "@ngrx/store-devtools": "^14.3.2",
    "firebase": "^9.19.1",
    "ngx-webstorage": "^10.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular-eslint/builder": "14.4.0",
    "@angular-eslint/eslint-plugin": "14.4.0",
    "@angular-eslint/eslint-plugin-template": "14.4.0",
    "@angular-eslint/schematics": "14.4.0",
    "@angular-eslint/template-parser": "14.4.0",
    "@angular/cli": "~14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@ngrx/eslint-plugin": "^14.3.2",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "ngx-build-plus": "^14.0.0",
    "standard": "^17.0.0",
    "typescript": "~4.8.4"
  }
